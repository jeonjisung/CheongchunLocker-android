        android:theme="@style/Theme.CCLK">
        <activity android:name=".Locker1"/>
        <activity android:name=".Locker2"/>
        <activity android:name=".Locker3"/>
        <activity android:name=".Locker4"/>
        <activity android:name=".Locker5"/>
        <activity android:name=".Locker6"/>
        <activity android:name=".Locker7"/>
        <activity android:name=".Locker8"/>
        <activity android:name=".Locker9"/>
        <activity android:name=".LockerMain"/>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>